{
  "name": "@tgallacher/ddd-ts",
  "version": "0.1.1",
  "description": "Domain driven design building blocks",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "Tom Gallacher",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgallacher/ddd-ts.git"
  },
  "homepage": "https://github.com/tgallacher/ddd-ts#readme",
  "bugs": {
    "url": "https://github.com/tgallacher/ddd-ts/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && vite build && tsc --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --watch"
  },
  "keywords": [
    "ddd",
