    "typescript",
    "entity",
    "value-object",
    "aggregate"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "fast-equals": "^5.2.2",
    "uuid": "^13.0.0"
  }
}
